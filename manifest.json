{
    "name" : "StudyFier",
    "version": "0.0.1",
    "manifest_version":3,
    "action": {
        "default_popup" : "popup.html"
    },
    "permissions": [
        "activeTab"
      ],
    "content_scripts" :[
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
        }
    ]
}